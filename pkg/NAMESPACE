useDynLib(vsa)
# Generics that have methods for subclasses of vsa
export(newVec, vsaprod, vsapower, appinv, dot, equiv, cosine, vnorm, mag, add, addnorm, vsascale, conformable, simval, scalar)
# Functions for vsa objects (don't need specific methods for subclsses)
export("%.%", "%==%", "%cos%", elts)
# Functions for working with 'vsamem' objects
export(vsamem, addmem, getmem, delmem, cosmem, cleanup, bestmatch, memsize, memlabels)
export(contents, setmem, memmags, dotmem, dotmem_vsamat_compute, getmemraw)

S3method(addmem, vsamat)
S3method(addmem, default)
S3method(addmem, vsadb)

S3method(cosmem, default)

S3method(cleanup, default)

S3method(bestmatch, default)

S3method(getmem, vsalist)
S3method(getmem, vsamat)
S3method(getmem, realhrr_ramdb)

S3method(getmemraw, realhrr_ramdb)

S3method(memsize, vsalist)

S3method(memlabels, vsalist)

S3method(memsize, vsamat)

S3method(memlabels, vsamat)

S3method(print, vsa)
S3method(print, vsalist)
S3method(print, vsamat)
S3method(print, realhrr_ramdb)
S3method(print, simval)
S3method(Ops, vsa)
S3method(elts, vsa)

S3method(newVec, default)

S3method(newVec, realhrr)
S3method(vsaprod, realhrr)
S3method(vsapower, realhrr)
S3method(appinv, realhrr)
S3method(dot, realhrr)
S3method(equiv, realhrr)
S3method(cosine, realhrr)
S3method(vnorm, realhrr)
S3method(mag, realhrr)
S3method(add, realhrr)
S3method(vsascale, realhrr)

S3method(newVec, drep)
S3method(vsaprod, drep)
S3method(vsapower, drep)
S3method(appinv, drep)
S3method(dot, drep)
S3method(equiv, drep)
S3method(cosine, drep)
S3method(vnorm, drep)
S3method(mag, drep)
S3method(add, drep)
S3method(vsascale, drep)

S3method(scalar, default)
S3method(scalar, simval)

S3method(contents, realhrr_ramdb)
S3method(contents, vsamat)

S3method(setmem, vsamat)
S3method(setmem, vsalist)
S3method(setmem, realhrr_ramdb)

S3method(memmags, default)
S3method(memmags, realhrr_ramdb)

S3method(dotmem, vsalist)
S3method(dotmem, vsamat)
S3method(dotmem, realhrr_ramdb)

S3method(dotmem_vsamat_compute, default)
S3method(dotmem_vsamat_compute, ihrr32)
S3method(dotmem_vsamat_compute, realhrr)
